build_go() {
	@(cd:"./go/")
	gomake build_nochecks
}

build_rust() {
	@(cd:"./rust/")
	gomake build
}

copy_executables() {
	copy go\PortableBuildTools.exe build\PortableBuildTools.exe
	copy rust\target\release\MSIExtract.exe build\MSIExtract.exe
}

build() {
	@build_go
	@build_rust
	@copy_executables
}